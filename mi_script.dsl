// Operaciones con potencias y funciones matemáticas
var base = 2;
var exponente = 3;
var potencia = base ^ exponente;
var raiz = sqrt(16);
var seno = sin(3.14); // Aproximación de pi
var coseno = cos(3.14); // Aproximación de pi
print("Potencia:", potencia);
print("Raíz cuadrada:", raiz);
print("Seno:", seno);
print("Coseno:", coseno);
